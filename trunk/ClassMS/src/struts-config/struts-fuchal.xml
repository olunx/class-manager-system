<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts 
	PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd" >
<struts>
	<!--  classfee action	-->
	<package name="classfee" extends="struts-default" namespace="/classfee">
		
		<global-results>
			<result name="input">/WEB-INF/model/classfee/classfeeAdd.jsp</result>
			<result name="error">/WEB-INF/model/classfee/classfeeErr.jsp</result>
			<result type="redirectAction" name="list">list</result>
		</global-results>
		
		<action name="save" class="cn.gdpu.action.ClassFeeAction" method="save">			
			<result name="addsuc">/WEB-INF/model/classfee/classfeeSuc.jsp?value=add</result>
			<result name="modifysuc">/WEB-INF/model/classfee/classfeeSuc.jsp?value=modify</result>
		</action> 
		
		<action name="delete" class="cn.gdpu.action.ClassFeeAction" method="delete">
			<result>/WEB-INF/model/classfee/classfeeSuc.jsp?value=delete</result>
		</action> 
		
		<action name="deleteMany" class="cn.gdpu.action.ClassFeeAction" method="deleteMany">
			<result>/WEB-INF/model/classfee/classfeeSuc.jsp?value=deleteMany</result>
		</action> 
				
		<action name="modifyLink" class="cn.gdpu.action.ClassFeeAction" method="modifyLink">
			<result>/WEB-INF/model/classfee/classfeeAdd.jsp</result>
		</action> 		
		
		<action name="query" class="cn.gdpu.action.ClassFeeAction" method="query">
			<result>/WEB-INF/model/classfee/classfeeDetail.jsp</result>
		</action> 
		
		<action name="list" class="cn.gdpu.action.ClassFeeAction" method="list">
			<result>/WEB-INF/model/classfee/classfee.jsp</result>
		</action> 
				
		<action name="addLink">
			<result>/WEB-INF/model/classfee/classfeeAdd.jsp</result>
		</action>
		
	</package>
	
	<!--  vote action	-->
	<package name="vote" extends="struts-default" namespace="/vote">
			
		<global-results>
			<result name="input">/WEB-INF/model/vote/voteErr.jsp</result>
			<result name="error">/WEB-INF/model/vote/voteErr.jsp</result>		
			<result type="redirectAction" name="list">list</result>
		</global-results>
		
		<action name="add" class="cn.gdpu.action.VoteAction" method="add">
			<result>/WEB-INF/model/vote/voteSuc.jsp?value=add</result>	
			<result name="input">/WEB-INF/model/vote/voteAdd.jsp</result>
		</action>
		
		<action name="delete" class="cn.gdpu.action.VoteAction" method="delete">
			<result>/WEB-INF/model/vote/voteSuc.jsp?value=delete</result>	
		</action>
		
		<action name="deleteMany" class="cn.gdpu.action.VoteAction" method="deleteMany">
			<result>/WEB-INF/model/vote/voteSuc.jsp?value=deleteMany</result>	
		</action>
		
		<action name="query" class="cn.gdpu.action.VoteAction" method="query">
			<result>/WEB-INF/model/vote/voteDetail.jsp</result>	
		</action>	
		
		<action name="list" class="cn.gdpu.action.VoteAction" method="list" >
			<result>/WEB-INF/model/vote/vote.jsp</result>
		</action>
		
		<action name="votingLink" class="cn.gdpu.action.VoteAction" method="votingLink" >
			<result>/WEB-INF/model/vote/voteLink.jsp</result>
		</action>		
		
		<action name="voting" class="cn.gdpu.action.VoteAction" method="voting">
			<result>/WEB-INF/model/vote/voteSuc.jsp?value=voting</result>
			<result name="input">/WEB-INF/model/vote/voteLink.jsp</result>
			<result name="voterexist">/WEB-INF/model/vote/voteErr.jsp?error=voterExist </result>
			<result name="timeout">/WEB-INF/model/vote/voteErr.jsp?error=timeout </result>
		</action>			
		
		<action name="addLink">
			<result>/WEB-INF/model/vote/voteAdd.jsp</result>
		</action>

	</package>
	
</struts>    