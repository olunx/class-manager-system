<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts 
	PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd" >
<struts>

	<package name="login" extends="struts-default">
		<action name="student" class="cn.gdpu.action.StudentAction">
			<result name="success">/regSuc.jsp</result>
			<result name="error">/regFail.jsp</result>
		</action>
		<action name="fileUpload" class="cn.gdpu.action.FileUploadAction">
			
			<interceptor-ref name="fileUpload">
				<param name="allowedTypes">application/vnd.ms-excel,application/msword,application/octet-stream</param>
				<param name="maximumSize">10485760</param>
			</interceptor-ref>
			<interceptor-ref name="defaultStack"></interceptor-ref>
			<result name="success">/uploadSuc.jsp</result>
			<result name="error">/regFail.jsp</result>
		</action>
	</package>

</struts>    