<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.gdpu.vo">
	<class name="Attendance" table="t_attendance">
		<id name="aid" column="aid" >
			<generator class="native" />
		</id>
		<set name="students" table="t_attendance_student" inverse="true" >
			<key column="aid" />
			<many-to-many class="Student" column="stuId" />
		</set>

		<!--
			<set name="sid" table="t_attendance_student" inverse="true" cascade="all"> <key column="aid" /> <many-to-many class="Student"
			column="stuId" /> </set>
		-->
		<many-to-one name="clerk" />
		<property name="subject" />
		<property name="time" />
		<property name="week" />
		<property name="day" />
		<property name="lesson" />
	</class>
</hibernate-mapping>
