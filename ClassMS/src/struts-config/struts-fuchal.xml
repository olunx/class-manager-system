<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts 
	PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd" >
<struts>

	<package name="classfee" extends="struts-default" namespace="/classfee">
		<action name="*classFee" class="cn.gdpu.action.ClassFeeAction" method="{1}">
			<result name="success">/WEB-INF/model/classfee/classfeeSuc.jsp?value={1}</result>
			<result name="queryall">/WEB-INF/model/classfee/classfee.jsp</result>
			<result name="query">/WEB-INF/model/classfee/classfeeDetail.jsp</result>
			<result name="error">/WEB-INF/model/classfee/classfeeFail.jsp</result>
			<result name="modifylink">/WEB-INF/model/classfee/classfeeModify.jsp</result>
		</action> 			
				
		<action name="addLink">
			<result>/WEB-INF/model/classfee/classfeeAdd.jsp</result>
		</action>
		
	</package>

	<package name="vote" extends="struts-default" namespace="/vote">
		<action name="*vote" class="cn.gdpu.action.VoteAction" method="{1}">
			<result name="success" >/WEB-INF/model/vote/voteSuc.jsp?value={1}</result>
			<result name="list">/WEB-INF/model/vote/vote.jsp</result>
			<result name="query">/WEB-INF/model/vote/voteDetail.jsp</result>
			<result name="modifylink">/WEB-INF/model/vote/voteModify.jsp</result>
			<result name="votinglink">/WEB-INF/model/vote/voteLink.jsp</result>
			<result name="voterexist">/WEB-INF/model/vote/voteErr.jsp?error=voterExist </result>
		</action>
		
		<action name="addLink">
			<result>/WEB-INF/model/vote/voteAdd.jsp</result>
		</action>

	</package>
	
</struts>    