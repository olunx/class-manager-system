<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.gdpu.vo">
	<class name="Student" table="t_student">
		<id name="stuId" column="stuId">
			<generator class="native" />
		</id>
		<many-to-one name="duty" column="dutyId" />
		<property name="avatar" />
		<property name="username" />
		<property name="password" />
		<property name="sno" type="string"/>
		<property name="realName" />
		<property name="dorm" />
		<property name="phoneNo" />
		<property name="qq" />
		<property name="mail" />
		<set name="activityApplys" inverse="true" cascade="all" lazy="false" order-by="aid asc">
			<key column="student_id"></key>
			<one-to-many class="ActivityApply"/>
		</set>
	</class>
</hibernate-mapping>
