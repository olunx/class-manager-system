<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.gdpu.vo">
	<class name="Student" table="t_student">
		<id name="id">
			<generator class="native" />
		</id>
		<property column="avatar" generated="never" lazy="false"
			name="avatar" not-null="false" />
		<property column="username" generated="never" lazy="false"
			name="username" not-null="false" />
		<property column="password" generated="never" lazy="false"
			name="password" not-null="false" />
		<property column="Sno" generated="never" lazy="false" name="Sno"
			not-null="false" />
		<property column="realName" generated="never" lazy="false"
			name="realName" not-null="false" />
		<property column="dorm" generated="never" lazy="false" name="dorm"
			not-null="false" />
		<property column="duty" generated="never" lazy="false" name="duty"
			not-null="false" />
		<property column="phoneNo" generated="never" lazy="false"
			name="phoneNo" not-null="false" />
		<property column="qq" generated="never" lazy="false" name="qq"
			not-null="false" />
		<property column="mail" generated="never" lazy="false" name="mail"
			not-null="false" />
	</class>
</hibernate-mapping>
