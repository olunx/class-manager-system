<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts 
	PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd" >
<struts>

	<package name="classfee" extends="struts-default" >
		<action name="*classFee" class="cn.gdpu.action.ClassFeeAction" method="{1}">
			<result name="success">/WEB-INF/model/classfee/classfee.jsp?value={1}</result>
			<result name="error">/WEB-INF/model/classfee/classfeeFail.jsp</result>
			<result name="input">/WEB-INF/model/classfee/classfee.jsp?value=input</result>
		</action> 					
	</package>

	<package name="vote" extends="struts-default" >
		<action name="*vote" class="cn.gdpu.action.VoteAction" method="{1}">
			<result name="success" >/voteSuc.jsp?value={1}</result>
		</action>
	</package>
	
	<package name="kissy" extends="struts-default" >
		<action name="kissyUpload" class="cn.gdpu.action.KissyUploadAction" >
			<param name="savePath">/content/images</param>
			<result name="success">/content/kissy/kissyRes.jsp?status=0</result>
			<result name="error">/content/kissy/kissyRes.jsp?status=1</result>
		</action>
	</package>
	
</struts>    